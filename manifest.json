{
  "name": "Auto-Redirect to NBViewer",
  "description": "Auto-Redirect jupyter notebooks to NbViewer",
  "version": "0.1",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "permissions": [
    "webRequest",
    "*://github.com/*",
    "*://www.github.com/*",
    "webRequestBlocking"
  ]
}
